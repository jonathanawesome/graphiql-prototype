{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,    
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "lib": [ "ESNext"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "strict": true,
    "target": "ESNext",
  }
}
